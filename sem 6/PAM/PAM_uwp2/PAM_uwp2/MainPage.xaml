<Page
    x:Class="PAM_uwp2.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PAM_uwp2"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="btnHome" Content="Home"  Margin="0, 0, 20, 0" Click="btnHome_Click"/>
                <Button x:Name="btnBack" Content="Back" Margin="0, 0, 20, 0" Click="btnBack_Click"/>
                <Button x:Name="btnForward" Content="Forward"  Margin="0, 0, 20, 0" Click="btnForward_Click"/>
            </StackPanel>
            <Frame Name="MyFrame">
                
            </Frame>
        </StackPanel>
    </Grid>
</Page>
